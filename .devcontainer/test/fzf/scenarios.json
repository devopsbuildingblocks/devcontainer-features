{
    "no_keybindings": {
        "image": "ghcr.io/devopsbuildingblocks/devcontainer-images/ubuntu-devbox:latest",
        "features": {
            "fzf": {
                "enableKeyBindings": false
            }
        }
    },
    "custom_options": {
        "image": "ghcr.io/devopsbuildingblocks/devcontainer-images/ubuntu-devbox:latest",
        "features": {
            "fzf": {
                "defaultOptions": "--height 50% --layout=reverse --border --preview 'cat {}'"
            }
        }
    },
    "dracula_theme": {
        "image": "ghcr.io/devopsbuildingblocks/devcontainer-images/ubuntu-devbox:latest",
        "features": {
            "fzf": {
                "theme": "dracula"
            }
        }
    }
}
