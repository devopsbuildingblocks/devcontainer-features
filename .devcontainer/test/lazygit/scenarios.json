{
    "custom_theme": {
        "image": "ghcr.io/devopsbuildingblocks/devcontainer-images/ubuntu-devbox:latest",
        "features": {
            "lazygit": {
                "theme": "dracula"
            }
        }
    },
    "with_pager": {
        "image": "ghcr.io/devopsbuildingblocks/devcontainer-images/ubuntu-devbox:latest",
        "features": {
            "lazygit": {
                "enableGitDiffPager": true
            }
        }
    },
    "with_file_icons": {
        "image": "ghcr.io/devopsbuildingblocks/devcontainer-images/ubuntu-devbox:latest",
        "features": {
            "lazygit": {
                "enableFileIcons": true
            }
        }
    }
}
