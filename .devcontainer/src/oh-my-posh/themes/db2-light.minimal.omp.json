{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/refs/tags/v26.23.6/themes/schema.json",
  "version": 3,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "#5F9E5F",
          "foreground_templates": [
            "{{ if .Error }}#B8202E{{ end }}"
          ],
          "template": "\u279c",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#C09020",
          "template": " {{ .Path }} ",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#B8202E",
          "template": "<#2B5A7D>git:(</>{{ .HEAD }}<#2B5A7D>)</>",
          "properties": {
            "branch_icon": ""
          }
        }
      ]
    }
  ],
  "final_space": true
}
